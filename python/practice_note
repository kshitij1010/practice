---->>>>> Count how many time single character is coming in string::
string =  raw_input("enter any thing::")
#set(string) = a,b,k,s,j
for i in set(string):
	if string.count(i)==1:
		print i

----->>>>>>Count how many time words are coming in list
string =  raw_input("enter any thing::")
#set(string) = a,b,k,s,j
s = "123123 123 345 67 45 857 3756 29835"
##a=s.split()

for i in set(string.split(" ")):

	if string.count(i)==1:
		print i
